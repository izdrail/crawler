{
    "name": "cornatul/crawler",
    "description": "A set of tools to make parsing websites easier",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Stefan I",
            "email": "Stefan@unxidevil.com",
            "homepage": "https://cornatul.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cornatul\\Crawler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cornatul\\Crawler\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": ">=7.5.0",
        "illuminate/support": ">=9.0.0",
        "illuminate/console": ">=v9.0.0",
        "spatie/laravel-data": ">=3.0.0",
        "symfony/css-selector": "^6.2",
        "symfony/browser-kit": "^6.2",
        "symfony/dom-crawler": "^6.2",
        "crwlr/crawler": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Cornatul\\Crawler\\CrawlerServiceProvider"
            ]
        }
    }
}
